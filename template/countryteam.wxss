.countryTeam {
  min-height: 240rpx;
  background: #fff;
  border-radius: 10rpx;
    box-shadow: 0rpx 7rpx 6rpx rgba(255,91,91,0.1);
  box-sizing: border-box;
  padding: 30rpx 0;
  position: relative;
  margin-bottom: 30rpx;
}

.leftC {
  height: 92rpx;
  width: 92rpx;
  position: absolute;
  top: 30rpx;
  left: 30rpx;
}

.rightC {
  height: 92rpx;
  width: 92rpx;
  position: absolute;
  top: 30rpx;
  right: 30rpx;
  border-radius: 50%;
}

.cInfoBox {
  height: 60rpx;
  line-height: 60rpx;
  text-align: center;
  font-size: 28rpx;
}

.cInfoBox .red {
  font-size: 46rpx;
  color: #ff5b5b;
  font-weight: bold;
  margin: 0 30rpx;
}

.timeBox {
  height: 40rpx;
  line-height: 40rpx;
  display: flex;
  justify-content: center;
  align-items: center;
  font-size: 28rpx;
  color: #bdbdbd;
  margin-top: 20rpx;
}

.timeBox image {
  width: 32rpx;
  height: 32rpx;
  margin-right: 10rpx;
}

.backUpBox {
  height: 70rpx;
  position: absolute;
  bottom: 30rpx;
  left: 0rpx;
  width: 100%;
}

.backUpBox {
  height: 70rpx;
  position: absolute;
  bottom: 30rpx;
  left: 0rpx;
  width: 100%;
}

.backUpBox .timeLeft {
  width: 50%;
  position: absolute;
  left: 30rpx;
  bottom: 0;
  height: 70rpx;
  display: flex;
  text-align: left;
  align-items: center;
  font-size: 28rpx;
  color: #bdbdbd;
}

.backUpBox .timeLeft image {
  width: 32rpx;
  height: 32rpx;
  margin-right: 10rpx;
}

.backUpBox .btnRight {
  width: 180rpx;
  height: 62rpx;
  line-height: 62rpx;
  position: absolute;
  right: 30rpx;
  border-radius: 31rpx;
  bottom: 4rpx;
  text-align: center;
  font-size: 28rpx;
  color: #fff;
  background: -webkit-linear-gradient(top, rgb(255, 83, 83), rgb(255, 123, 123));
}
.checkBox{
    box-shadow: 0rpx 7rpx 6rpx rgba(255,91,91,0.1);  
}
.checkBox .checkTopBox {
  height: 70rpx;
  overflow: hidden;
  clear: both;
  text-align: center;
  line-height: 70rpx;
  font-size: 32rpx;
  color: #999;
  background: #fff;
}

.checkBox  .checkTopBox .checkTab {
  width: 33.3%;
  float: left;
  height: 70rpx;
  position: relative;
}



.guessingBox {
  padding: 40rpx 30rpx 0;
}

.guess {
  height: 70rpx;
  display: flex;
  justify-content: space-between;
  align-items: center;
}

.guess input {
  height: 70rpx;
  line-height: 70rpx;
  border-bottom: 1px solid #e9e9e9;
  width: 40%;
  text-align: center;
  font-size: 28rpx;
}

.suspect {
  height: 70rpx;
  display: flex;
  justify-content: space-between;
  align-items: center;
}

.suspect view {
  height: 62rpx;
  line-height: 62rpx;
  background: -webkit-linear-gradient(top, rgb(221, 221, 221), rgb(208, 208, 208));
  width: 30%;
  text-align: center;
  font-size: 28rpx;
  color: #666;
  border-radius: 2px;
}

.suspect .active {
  background: -webkit-linear-gradient(top, rgb(255, 83, 83), rgb(255, 123, 123));
  color: #fff;
}

.guessView {
  font-size: 28rpx;
  color: #666;
  margin-bottom: 20rpx;
}
.upfoldBox{
  margin-top: 30rpx;
}



.upBox {
  height: 80rpx;
  padding: 30rpx;
  clear: both;
  overflow: hidden;
}

.upBox .postGuess {
  height: 78rpx;
  line-height: 78rpx;
  background: -webkit-linear-gradient(top, rgb(255, 83, 83), rgb(255, 123, 123));
  color: #fff;
  width: 65%;
  border-radius: 40rpx;
  text-align: center;
  font-size: 32rpx;
  float: right;
}

.upBox .up {
  height: 76rpx;
  line-height: 76rpx;
  width: 30%;
  border: 1px solid #ff5b5b;
  color: #ff5b5b;
  border-radius: 40rpx;
  text-align: center;
  font-size: 32rpx;
  float: left;
}
.infoTest{
  color: #FF5B5B;
  font-size: 24rpx;
  overflow: hidden;
  text-align: center;
  padding: 0 30rpx;
}
.hasCheck{
  background: -webkit-linear-gradient(top, rgb(17, 123, 230), rgb(45, 149, 255)) !important;  
}
.c_white{
  color: #fff !important;  
}
.not_chose{
  background: -webkit-linear-gradient(top, rgb(221, 221, 221), rgb(208, 208, 208)) !important;      
}