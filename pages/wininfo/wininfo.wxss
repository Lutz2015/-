.winInfoBox{
  background: #fff;
}
.winInfoList{
  height: 144rpx;
  border-bottom: 1px solid #e9e9e9;
  margin-left: 30rpx;
  display: flex;
  justify-content: space-between;
  align-items: center;
  padding-right:30rpx;
  padding-left: 118rpx;
  position: relative; 
}
.userImg{
  width: 88rpx;
  height: 88rpx;
  position: absolute;
  left: 0;
  top: 50%;
  margin-top: -44rpx;
}
.userImg image{
  width: 100%;
  height: 100%;
  border-radius: 50%;
}
.userName{
  font-size: 28rpx;
  width: 50%;
  overflow: hidden;
  text-align: left;
}
.goodsInfo{
  width: 50%;
  height: 144rpx;
  overflow: hidden;
  text-align: right;
}
.goodsInfo .goodsName{
  font-size: 28rpx;
  margin:30rpx 0 10rpx;
}
.goodsInfo .goodsTime{
  font-size: 24rpx;
  color: #999;
}
 