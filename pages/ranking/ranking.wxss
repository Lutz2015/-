.realtimeTab{
  height: 70rpx;
  overflow: hidden;
  clear: both;
  text-align: center;
  line-height: 70rpx;
  font-size: 32rpx;
  color: #999;
  background: #fff;
  box-shadow: 0rpx 7rpx 6rpx rgba(255,91,91,0.1);
  position: fixed;
  width: 100%;
}
.realtimeTab .checkTab {
  width: 50%;
  height: 70rpx;
  position: relative;
  float: left;
}

.active {
  color: #ff5b5b;
}

.activeImg {
  width: 92rpx;
  height: 16rpx;
  position: absolute;
  bottom: -6rpx;
  left: 50%;
  margin-left: -46rpx;
}
.corerank,.invete{background: #fff}
.list{
  height: 144rpx;
  border-bottom: 1px solid #e9e9e9;
  margin-left: 30rpx;
  padding-right:30rpx;
  position: relative;
  padding: 30rpx 0;
  box-sizing: border-box;
  display: flex;
  align-items: center;
  font-size: 28rpx; 
}
.rank{
 width: 80rpx;
 overflow: hidden;
}
.userHead{
   width: 90rpx;
   height: 90rpx;
   margin-right: 30rpx;
   border-radius: 50%;
}
.userName{
  width: 300rpx;
  overflow: hidden;
}
.core{
  position: absolute;
  right: 30rpx;
  height: 100%;
  line-height: 144rpx;
  top: 0;
}
