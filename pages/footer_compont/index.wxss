.footer {
  height: 140rpx;
  width: 100%;
  background: #fff;
  position: fixed;
  bottom: 0;
  left: 0;
  display: flex;
  justify-content: space-around;
  align-items: center;
}

.iconBox {
  height: 140rpx;
  width: 20%;
  display: flex;
  flex-direction: column;
  align-items: center;
  justify-content: center;
  color: #949494;
}

.icon {
  width: 60rpx;
  height: 60rpx;
}

.iconBox text {
  font-size: 28rpx;
  margin-top: 6rpx;
}

.active {
  color: #97845e;
}
