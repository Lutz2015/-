.realtimeTab{
  height: 70rpx;
  overflow: hidden;
  clear: both;
  text-align: center;
  line-height: 70rpx;
  font-size: 32rpx;
  color: #999;
  background: #fff;
  box-shadow: 0rpx 6rpx 6rpx #ddd;
  position: fixed;
  width: 100%;
}
.realtimeTab .checkTab {
  width: 50%;
  height: 70rpx;
  position: relative;
  float: left;
}

.active {
  color: #ff5b5b;
}

.activeImg {
  width: 92rpx;
  height: 16rpx;
  position: absolute;
  bottom: -6rpx;
  left: 50%;
  margin-left: -46rpx;
}
.list{
  height: 144rpx;
  border-bottom: 1px solid #e9e9e9;
  margin-left: 30rpx;
  padding-right:30rpx;
  position: relative;
  padding: 30rpx 0;
  box-sizing: border-box;
}
.cname{
  font-size: 32rpx;
  line-height: 54rpx;
}
.time{
   font-size: 24rpx;
  line-height: 30rpx;
  color: #999;
}
.core{
  position: absolute;
  right: 30rpx;
  top: 0;
  height: 100%;
  line-height: 144rpx;
  color: #FF5B5B;
}