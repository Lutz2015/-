view, text, button, input, label, navigator {
  font-family: 'Hiragino Sans GB',
                 'Hiragino Sans GB W3',
                 'Microsoft YaHei',
                 '微软雅黑',
                 tahoma,
                 arial,
                 simsun,
                 '宋体';
}

page {
  width: 100%;
  height: 100%;
  background: #f5f5f5;
  position: relative;
}
/* 启动页 */
.loginView{
  height: 100%;
  width: 100%;
  position: fixed;
  top: 0;
  left: 0;
  z-index: 99999;
}
.loginView .loginBg{
  height: 100%;
  width:100%;
}
.loginView .agreebutton{
  width: 430rpx;
  height: 138rpx;
  position: absolute;
  bottom: 100rpx;
  left: 50%;
  margin-left: -215rpx;
}
.loginView .agreebutton image{
  width: 100%;
  height: 100%;
}
.loginView .agreebutton button{
  position: absolute;
  left: 0;
  top: 0;
  width: 100%;
  height: 100%;
  opacity: 0;
}
/* 启动页结束 */
.footer {
  height: 100rpx;
  width: 100%;
  background: #fff;
  position: fixed;
  bottom: 0;
  left: 0;
  display: flex;
  justify-content: space-around;
  align-items: center;
  z-index: 888;
}

.footer .iconBox {
  height: 100rpx;
  width: 20%;
  display: flex;
  flex-direction: column;
  align-items: center;
  justify-content: center;
  color: #999999;
}

.footer .iconBox .icon {
  width: 48rpx;
  height: 48rpx;
}

.footer .iconBox text {
  font-size: 24rpx;
  margin-top: 6rpx;
}

.footer .iconBox .active {
  color: #FF5B5B;
}
/* 底部tab结束 */
.pubCover{
  position: fixed;
  height: 100%;
  width: 100%;
  background: rgba(0, 0, 0, 0.2);
  bottom: 0;
  right: 0;
  z-index: 999;
}
/* 通用遮罩 层级999 */
.botomBlock {
  height: 100rpx;
}
.noMore{
  font-size: 32rpx;
  text-align: center;
  color: #999999;
  height: 80rpx;
  line-height: 80rpx;
}